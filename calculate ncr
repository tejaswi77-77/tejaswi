import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	 public static long factorial(int n){
    long p=1;
    for(int i=n;i>=1;i--){
      p*=i;
    }
    return p;
  }
	public static void main (String[] args) throws java.lang.Exception
	{
  	Scanner sc= new Scanner (System.in);
      int n = sc.nextInt();
      int r = sc.nextInt();
      long ncr=(factorial(n))/(factorial(n-r)*factorial(r));
      System.out.println(ncr);
	}
}
