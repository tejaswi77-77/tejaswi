import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
  {
      Scanner sc=new Scanner(System.in);
      long n=sc.nextLong();
		while(n!=0)
          {
            long dig=n%10;
            System.out.print(dig);
            n=n/10;
          }
        }
	}
