import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
    long f=1;
      for(int i=1;i<=n;i++)
        {
        f=f*i;//your code here
	}
      System.out.println(f);
}
  }
