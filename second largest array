import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
	Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int arr[]=new int[n];
      for(int i=0;i<n;i++) {
       arr[i]=sc.nextInt();//your code here
	}
      Arrays.sort(arr);
      System.out.println(arr[n-2]);
        }
      }
        
